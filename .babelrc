{
  "presets": [
    ["env", { "modules": false }],
    "es2015",
    "stage-2"
  ],
  "plugins": [
    ["transform-runtime", {
        "helpers": true,
        "polyfill": false,
        "regenerator": false,
        "moduleName": "babel-runtime"
      }
    ]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "es2015", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
